{
  "ConnectionStrings": {
    "PostgresConnection": "Host=localhost;Port=5432;Database=habitsdb;Username=postgres;Password=postgres"
  },
  "ThreadPoolSettings": {
    "MinWorkerThreads": 8,
    "MinCompletionPortThreads": 8,
    "MaxWorkerThreads": 50,
    "MaxCompletionPortThreads": 50
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByExcluding",
                "Args": {
                  "expression": "Contains(@m, 'BENCHMARK')"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "logs/app.log",
                  "rollingInterval": "Day",
                  "retainedFileCountLimit": 7,
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}",
                  "restrictedToMinimumLevel": "Error"
                }
              }
            ]
          }
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "Contains(@m, 'BENCHMARK')"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "logs/benchmark.log",
                  "rollingInterval": "Day",
                  "retainedFileCountLimit": 7,
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Enable_benchmark": true
}